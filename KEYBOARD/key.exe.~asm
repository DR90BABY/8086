DATA SEGMENT
PORTA EQU 00H
PCW   EQU 06H
ENDS

STACK SEGMENT
DW   128  DUP(0)
ENDS

CODE SEGMENT
START:
MOV AX, DATA
MOV DS, AX
MOV ES, AX

MOV DX,PCW
MOV AL,10000000B
OUT DX,AL

MOV AL,27
CALL OUTA
;MOV AH, 0H
;INT 16H
;CALL OUTA

HLT
PROC OUTA
;PARAMS: AL
PUSH DX
MOV DX,PORTA
OUT DX,AL
POP DX
RET
ENDP OUTA
CODE ENDS
END START



